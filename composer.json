{
    "name": "Arnonm/DDBlueprintAddon",
    "description": "Generate Domain files from your laravel blueprint!",
    "keywords": [
        "arnonm",
        "laravel-shift",
        "blueprint",
        "pest",
        "pestphp"
    ],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnon Meshoulam",
            "email": "arnonm.dev@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "jasonmccreary/laravel-test-assertions": "^2.1",
        "laravel-shift/blueprint": "^2.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21.1",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DDBlueprintAddon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arnon\\DDBlueprintAddon\\BlueprintDDAddonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
