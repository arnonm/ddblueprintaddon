{
    "name": "arnonm/ddblueprintaddon",
    "type": "project",
    "description": "Generate Domain files from your laravel blueprint!",
    "keywords": [
        "arnonm",
        "laravel-shift",
        "blueprint",
        "pest",
        "pestphp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arnon Meshoulam",
            "email": "arnonm.dev@gmail.com",
            "homepage": "https://github.com/arnonm",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "jasonmccreary/laravel-test-assertions": "^2.1",
        "laravel-shift/blueprint": "@dev"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21.1",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Arnonm\\DDBlueprintAddon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnonm\\DDBlueprintAddon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arnon\\DDBlueprintAddon\\BlueprintDDAddonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../test_of_shift_blueprint/package/blueprint"

        }
    ]
}
